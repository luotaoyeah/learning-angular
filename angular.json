{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    }
  },
  "defaultProject": "learning-angular",
  "newProjectRoot": "projects",
  "projects": {
    "learning-angular": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "hmr": {
              "budgets": [
                {
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hmr.ts"
                }
              ]
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "20mb",
                  "maximumWarning": "10mb",
                  "type": "initial"
                },
                {
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "options": {
            "allowedCommonJsDependencies": ["@ant-design/colors", "jquery", "jquery-ui/ui/widgets/slider.js", "lodash", "rxjs/internal/observable/of"],
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              { "glob": "**/*", "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/", "output": "/assets/" }
            ],
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "dist",
            "polyfills": "src/polyfills.ts",
            "resourcesOutputPath": "img",
            "scripts": ["node_modules/ajv/dist/ajv.bundle.js", { "input": "node_modules/document-register-element/build/document-register-element.js" }],
            "styles": ["src/index.less"],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "learning-angular:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": ["**/node_modules/**"],
            "tsConfig": ["tsconfig.app.json", "tsconfig.spec.json"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "hmr": {
              "browserTarget": "learning-angular:build:hmr",
              "hmr": true
            },
            "production": {
              "browserTarget": "learning-angular:build:production"
            }
          },
          "options": {
            "browserTarget": "learning-angular:build",
            "port": 3010,
            "proxyConfig": "proxy.conf.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": ["src/favicon.ico", "src/assets"],
            "browsers": "ChromeHeadlessNoSandbox",
            "codeCoverage": true,
            "codeCoverageExclude": ["src/app/core/api/**/*"],
            "karmaConfig": "src/karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "progress": false,
            "scripts": ["node_modules/ajv/dist/ajv.bundle.js"],
            "styles": ["src/index.less"],
            "tsConfig": "tsconfig.spec.json",
            "watch": false
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        }
      },
      "sourceRoot": "src"
    },
    "learning-angular-e2e": {
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "learning-angular:serve:production"
            }
          },
          "options": {
            "devServerTarget": "learning-angular:serve",
            "port": 3011,
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": ["**/node_modules/**"],
            "tsConfig": "e2e/tsconfig.e2e.json"
          }
        }
      },
      "prefix": "",
      "projectType": "application",
      "root": "e2e/"
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "flat": false,
      "inlineStyle": true,
      "inlineTemplate": false,
      "skipTests": false
    },
    "@schematics/angular:directive": {
      "skipTests": false
    },
    "@schematics/angular:module": {
      "routing": true,
      "skipTests": false
    },
    "@schematics/angular:service": {
      "skipTests": false
    }
  },
  "version": 1
}
