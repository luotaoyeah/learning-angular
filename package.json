    "lint-staged": "^8.1.7",
    "ng-alain": "^7.3.2",
    "ng-alain-codelyzer": "^0.0.1",
    "prettier": "^1.17.1",
    "protractor": "~5.4.0",
    "puppeteer": "^1.16.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.4.5"
  }
