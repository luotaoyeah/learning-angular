{
  "name": "learning-angular",
  "version": "0.0.1",
  "author": "luotao",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run color-less && ng serve",
    "build": "npm run color-less && ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --watch=false --progress=false --browsers=ChromeHeadlessNoSandbox --code-coverage",
    "test:coverage": "ng test --code-coverage --watch=false",
    "e2e": "ng e2e --port 42001",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor.ci.conf.js --port 42001",
    "analyze": "npm run color-less && ng build --prod --build-optimizer --stats-json",
    "color-less": "node scripts/color-less.js",
    "icon": "ng g ng-alain:plugin icon",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -c tslint.json src/**/*.ts",
    "lint:style": "stylelint src/**/*.less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "hmr": "ng serve --configuration=hmr"
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.4",
    "@antv/g2-plugin-slider": "^2.1.1",
    "@delon/abc": "^7.3.2",
    "@delon/acl": "^7.3.2",
    "@delon/auth": "^7.3.2",
    "@delon/cache": "^7.3.2",
    "@delon/chart": "^7.3.2",
    "@delon/form": "^7.3.2",
    "@delon/mock": "^7.3.2",
    "@delon/theme": "^7.3.2",
    "@delon/util": "^7.3.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "ajv": "^6.9.2",
    "angular-in-memory-web-api": "^0.8.0",
    "core-js": "^2.5.4",
    "husky": "^1.3.1",
    "ng-zorro-antd": "^7.3.3",
    "prettier-stylelint": "^0.4.2",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.3",
    "rxjs": "~6.3.3",
    "screenfull": "^4.2.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "tslib": "^1.9.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@angularclass/hmr": "^2.1.3",
    "@delon/testing": "^7.3.2",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.14.6",
    "chalk": "^2.4.2",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
